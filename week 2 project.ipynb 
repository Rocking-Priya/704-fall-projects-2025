      "outputs": [],
      "source": [
        "# YOUR CHANGES HERE\n",
        "\n",
        "import matplotlib.pyplot as plt\n",
        "import numpy as np\n",
        "from sklearn.model_selection import train_test_split\n",
        "from sklearn.linear_model import LinearRegression\n",
        "from sklearn.metrics import mean_squared_error\n"
      ]
    },
    {
      "cell_type": "code",
      "source": [
        "# Convert the 'month' column to datetime for easier date operations\n",
        "df['month'] = pd.to_datetime(df['month'])\n",
        "\n",
        "# Extract year and month number\n",
        "df['year'] = df['month'].dt.year\n",
        "df['month_num'] = df['month'].dt.month\n",
        "\n",
        "# Split into training (2020–2023) and testing (2024)\n",
        "train = df[df['year'] <= 2023]\n",
        "test = df[df['year'] == 2024]\n",
        "\n",
        "# Compute average price for each calendar month (seasonality model)\n",
        "monthly_avg = train.groupby('month_num')['price'].mean()\n",
        "\n",
        "# Predict 2024 using those averages\n",
        "predictions = test.copy()\n",
        "predictions['predicted_price'] = predictions['month_num'].map(monthly_avg)\n",
        "\n",
        "# Keep only month and predicted price\n",
        "backtest = predictions[['month', 'price']]\n",
        "\n",
        "# Save to TSV\n",
        "backtest.to_csv(\"strawberry-backtest.tsv\", sep=\"\\t\", index=False)\n",
        "\n",
        "print(backtest)"
      ],
      "metadata": {
